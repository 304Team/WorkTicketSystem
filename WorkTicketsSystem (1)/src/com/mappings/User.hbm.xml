<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.bean.User" table="USER">
          <id name="id" column="id" type="string"></id>
          <property name="name" column="name" type="string" />
          <property name="password" column="password" type="string"></property>
          <property name="add" column="add" type="int"></property>
          <property name="delete" column="delete" type="int"></property>
          <property name="query" column="query" type="int"></property>
          <property name="ieport" column="ieport" type="int"></property>
          <property name="statistics" column="statistics" type="int"></property>
 </class>
</hibernate-mapping>

